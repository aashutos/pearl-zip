<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="415.0" prefWidth="428.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ntak.pearlzip.ui.pub.FrmOptionsController">
    <TabPane fx:id="tabPaneOptions" prefHeight="364.0" prefWidth="428.0" tabClosingPolicy="UNAVAILABLE">
        <Tab fx:id="tabGeneral" text="%options.general.text">
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="315.0" prefWidth="428.0">
                <Button fx:id="btnClearCache" layoutX="23.0" layoutY="74.0" mnemonicParsing="false" text="%options.general.btnClearCache.text" />
                <Label alignment="TOP_LEFT" layoutX="23.0" layoutY="14.0" prefHeight="52.0" prefWidth="391.0" text="%options.general.description.text" wrapText="true" />
            <TitledPane fx:id="titlePaneStartUpOptions" collapsible="false" layoutX="14.0" layoutY="129.0" prefHeight="148.0" prefWidth="403.0" text="%options.general.startUpOptions.text" />
                <Label fx:id="lblDefaultFormat" layoutX="23.0" layoutY="169.0" text="%options.general.defaultFormat.text" />

                      <ComboBox fx:id="comboDefaultFormat" layoutX="264.0" layoutY="164.0" prefHeight="27.0" prefWidth="142.0" promptText="zip" />

                                                          <CheckBox fx:id="checkShowNotification" layoutX="23.0" layoutY="209.0" mnemonicParsing="false" text="%options.general.checkShowNotifications.text" />
                <CheckBox fx:id="checkSafeMode" layoutX="23.0" layoutY="239.0" mnemonicParsing="false" text="%options.general.checkSafeMode.text" />
            </AnchorPane>
        </Tab>
        <Tab fx:id="tabBootstrap" text="%options.bootstrap-properties.text">
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                <TableView fx:id="tblBootstrap" layoutX="10.0" layoutY="15.0" prefHeight="300.0" prefWidth="410.0">
                    <columns>
                        <TableColumn fx:id="key" prefWidth="211.0" text="%options.bootstrap-properties.key.text" />
                        <TableColumn fx:id="value" minWidth="0.0" prefWidth="194.0" text="%options.bootstrap-properties.value.text" />
                    </columns>
                </TableView>
            </AnchorPane>
        </Tab>
        <Tab fx:id="tabProviders" text="%options.providers.text">
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="428.0">
                <TableView fx:id="tblProviders" layoutX="10.0" layoutY="15.0" prefHeight="239.0" prefWidth="410.0">
                    <columns>
                        <TableColumn fx:id="name" prefWidth="109.0" text="%options.providers.name.text" />
                        <TableColumn fx:id="readCapability" prefWidth="68.0" text="%options.providers.name.readCapability.text" />
                        <TableColumn fx:id="writeCapability" prefWidth="68.0" text="%options.providers.name.writeCapability.text" />
                        <TableColumn fx:id="priority" prefWidth="68.0" text="%options.providers.name.priority.text" />
                        <TableColumn fx:id="supportedFormat" prefWidth="163.0" text="%options.providers.name.supportedFormat.text" />
                    </columns>
                </TableView>
            <Button fx:id="btnPurgePlugin" layoutX="10.0" layoutY="261.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="107.0" text="%options.providers.purge-plugin.text" />
            <Button fx:id="btnPurgeAll" layoutX="125.0" layoutY="261.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="141.0" text="%options.providers.purge-all.text" />
            </AnchorPane>
        </Tab>
        <Tab fx:id="tabPluginLoader" text="%options.plugin-loader.text">
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                <Pane fx:id="paneDropArea" layoutX="14.0" layoutY="73.0" prefHeight="246.0" prefWidth="398.0" style="-fx-border-color: black; -fx-background-image: url('drop-area.png');" />
                <Label fx:id="lblPluginLoaderDescriptionlblPluginLoaderDescription" alignment="TOP_LEFT" layoutX="14.0" layoutY="14.0" prefHeight="58.0" prefWidth="399.0" text="%options.plugin-loader.description.text" wrapText="true" />
            </AnchorPane>
        </Tab>
    </TabPane>
    <ButtonBar layoutY="364.0" prefHeight="51.0" prefWidth="428.0">
        <buttons>
            <Button fx:id="btnOk" mnemonicParsing="false" text="%options.btnOk.text" />
            <Button fx:id="btnApply" mnemonicParsing="false" text="%options.btnApply.text" />
            <Button fx:id="btnCancel" cancelButton="true" mnemonicParsing="false" text="%options.btnCancel.text" translateX="-5.0" />
        </buttons>
    </ButtonBar>
</AnchorPane>

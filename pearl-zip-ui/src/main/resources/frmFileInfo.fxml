<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="506.0" prefWidth="368.0" xmlns="http://javafx.com/javafx/17.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ntak.pearlzip.ui.pub.FrmFileInfoController">
    <VBox prefHeight="506.0" prefWidth="368.0">
        <Accordion prefHeight="465.0" prefWidth="368.0" expandedPane="$tpGeneral">
            <panes>
                <TitledPane fx:id="tpGeneral" animated="false" text="%file-info.general.text">
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                        <GridPane fx:id="grdGeneral" layoutX="14.0" layoutY="14.0" prefHeight="326.0"
                                  prefWidth="339.0">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                            </rowConstraints>
                            <Label fx:id="lblIndex" text="%file-info.general.index.text"/>
                            <Label fx:id="lblIndexValue" alignment="CENTER_RIGHT"
                                   contentDisplay="CENTER" prefHeight="17.0" prefWidth="172.0" text="-"
                                   textAlignment="CENTER" GridPane.columnIndex="1"/>
                            <Label fx:id="lblLevel" text="%file-info.general.level.text"
                                   GridPane.rowIndex="1"/>
                            <Label fx:id="lblLevelValue" alignment="CENTER_RIGHT"
                                   contentDisplay="CENTER" prefHeight="17.0" prefWidth="185.0" text="-"
                                   GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                            <Label fx:id="lblFileName" text="%file-info.general.filename.text"
                                   GridPane.rowIndex="2"/>
                            <Label fx:id="lblFilenameValue" alignment="CENTER_RIGHT"
                                   contentDisplay="CENTER" prefHeight="23.0" prefWidth="169.0" text="-"
                                   wrapText="true" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                            <Label fx:id="lblHash" alignment="CENTER" contentDisplay="CENTER"
                                   text="%file-info.general.hash.text" GridPane.rowIndex="3"/>
                            <Label fx:id="lblHashValue" alignment="CENTER_RIGHT" contentDisplay="CENTER"
                                   prefHeight="17.0" prefWidth="184.0" text="-" GridPane.columnIndex="1"
                                   GridPane.rowIndex="3"/>
                            <Label fx:id="lblRawSize" text="%file-info.general.rawSize.text"
                                   GridPane.rowIndex="5"/>
                            <Label fx:id="lblRawSizeValue" alignment="CENTER_RIGHT"
                                   contentDisplay="CENTER" prefHeight="27.0" prefWidth="168.0" text="-"
                                   GridPane.columnIndex="1" GridPane.rowIndex="5"/>
                            <Label fx:id="lblPackedSize" text="%file-info.general.packedSize.text"
                                   GridPane.rowIndex="4"/>
                            <Label fx:id="lblPackedSizeValue" alignment="CENTER_RIGHT"
                                   contentDisplay="CENTER" prefHeight="28.0" prefWidth="169.0" text="-"
                                   GridPane.columnIndex="1" GridPane.rowIndex="4"/>
                            <Label fx:id="lblFolder" text="%file-info.general.folder.text"
                                   GridPane.rowIndex="6"/>
                            <Label fx:id="lblEncrypt" text="%file-info.general.encrypt.text"
                                   GridPane.rowIndex="7"/>
                            <Label fx:id="lblComments" text="%file-info.general.comments.text"
                                   GridPane.rowIndex="8"/>
                            <Label fx:id="lblFolderValue" alignment="CENTER_RIGHT"
                                   contentDisplay="CENTER" prefHeight="31.0" prefWidth="169.0" text="-"
                                   GridPane.columnIndex="1" GridPane.rowIndex="6"/>
                            <Label fx:id="lblEncryptValue" alignment="CENTER_RIGHT"
                                   contentDisplay="CENTER" prefHeight="27.0" prefWidth="169.0" text="-"
                                   GridPane.columnIndex="1" GridPane.rowIndex="7"/>
                            <Label fx:id="lblCommentsValue" alignment="CENTER_RIGHT"
                                   contentDisplay="CENTER" prefHeight="39.0" prefWidth="169.0" text="-"
                                   GridPane.columnIndex="1" GridPane.rowIndex="8"/>
                        </GridPane>
                    </AnchorPane>
                </TitledPane>
                <TitledPane fx:id="tpTimestamps" animated="false" text="%file-info.timestamp.text">
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                        <GridPane fx:id="grdTimestamps" layoutX="14.0" layoutY="14.0" prefHeight="116.0"
                                  prefWidth="339.0">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                            </rowConstraints>
                            <Label fx:id="lblLastWriteTime" text="%file-info.timestamp.writeTime.text"/>
                            <Label fx:id="lblLastWriteTimeValue" alignment="CENTER"
                                   contentDisplay="CENTER" prefHeight="17.0" prefWidth="172.0" text="-"
                                   textAlignment="CENTER" GridPane.columnIndex="1"/>
                            <Label fx:id="lblLastAccessTime" text="%file-info.timestamp.accessTime.text"
                                   GridPane.rowIndex="1"/>
                            <Label fx:id="lblLastAccessTimeValue" alignment="CENTER"
                                   contentDisplay="CENTER" prefHeight="17.0" prefWidth="185.0" text="-"
                                   GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                            <Label fx:id="lblCreateTime" text="%file-info.timestamp.createTime.text"
                                   GridPane.rowIndex="2"/>
                            <Label fx:id="lblCreateTimeValue" alignment="CENTER" contentDisplay="CENTER"
                                   prefHeight="23.0" prefWidth="169.0" text="-" wrapText="true"
                                   GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                        </GridPane>
                    </AnchorPane>
                </TitledPane>
                <TitledPane fx:id="tpOwnership" animated="false" text="%file-info.ownership.text">
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                        <GridPane fx:id="grdOwnership" layoutX="14.0" layoutY="14.0" prefHeight="68.0"
                                  prefWidth="339.0">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                            </rowConstraints>
                            <Label fx:id="lblUser" text="%file-info.ownership.user.text"/>
                            <Label fx:id="lblUserValue" alignment="CENTER" contentDisplay="RIGHT"
                                   prefHeight="17.0" prefWidth="172.0" text="-" textAlignment="CENTER"
                                   GridPane.columnIndex="1"/>
                            <Label fx:id="lblGroup" text="%file-info.ownership.group.text"
                                   GridPane.rowIndex="1"/>
                            <Label fx:id="lblGroupValue" alignment="CENTER" contentDisplay="RIGHT"
                                   prefHeight="17.0" prefWidth="185.0" text="-" GridPane.columnIndex="1"
                                   GridPane.rowIndex="1"/>
                        </GridPane>
                    </AnchorPane>
                </TitledPane>
                <TitledPane fx:id="tpOther" animated="false" layoutX="10.0" layoutY="64.0" text="%file-info.other.text">
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                        <TableView fx:id="tblOtherInfo" layoutX="14.0" layoutY="14.0" prefHeight="329.0"
                                   prefWidth="342.0">
                            <columns>
                                <TableColumn fx:id="key" editable="false" prefWidth="75.0" sortable="false"
                                             text="Key"/>
                                <TableColumn fx:id="value" editable="false" prefWidth="75.0"
                                             sortable="false" text="Value"/>
                            </columns>
                            <columnResizePolicy>
                                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                            </columnResizePolicy>
                        </TableView>
                    </AnchorPane>
                </TitledPane>
            </panes>
        </Accordion>
        <ToolBar nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" prefWidth="200.0">
            <Button fx:id="btnClose" text="%file-info.btnClose.text"/>
        </ToolBar>
    </VBox>
</AnchorPane>
